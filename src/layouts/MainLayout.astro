---
import NavBar from "../components/Navbar.astro";

interface Props {
  title: string;
}

const { title } = Astro.props;
---

<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <meta name="generator" content={Astro.generator} />
    <link href="/src/output.css" rel="stylesheet">
    <title>{ title }</title>
  </head>
  <body>
    <NavBar />
    <slot />
  </body>
</html>

